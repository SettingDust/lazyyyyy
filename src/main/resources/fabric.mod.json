{
    "schemaVersion": 1,
    "id": "${id}",
    "version": "${version}",
    "name": "${name}",
    "description": "${description}",
    "authors": [
        "${author}"
    ],
    "contact": {
        "homepage": "${source}",
        "sources": "${source}",
        "issues": "${source}/issues"
    },
    "license": "MIT",
    "icon": "assets/${id}/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            {
                "adapter": "kotlin",
                "value": "settingdust.lazyyyyy.EntrypointKt::init"
            }
        ]
    },
    "depends": {
        "fabricloader": "${fabric_loader}",
        "minecraft": "${minecraft}",
        "fabric-language-kotlin": "${fabric_kotlin}"
    },
    "mixins": ["${id}.mixins.json"]
}
